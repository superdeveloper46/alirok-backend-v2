{
  "name": "rok-api-v2",
  "version": "2.0.0",
  "description": "",
  "author": "Alirok",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "npm run copy:assets && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma": "prisma",
    "copy:assets": "cpx 'src/generated/**' 'dist/generated'"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.1.2",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/websockets": "^8.1.2",
    "@ntegral/nestjs-sentry": "^3.0.6",
    "@prisma/client": "^3.13.0",
    "@sendgrid/mail": "^7.4.6",
    "@sentry/node": "^6.14.1",
    "@types/qrcode": "^1.4.2",
    "aws-sdk": "^2.1048.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "csv-parse": "^5.0.4",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "pdf-lib": "^1.17.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^8.174.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/html-pdf": "^2.2.1",
    "@types/jest": "^27.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cpx": "^1.5.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "prisma": "^3.13.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
