  drop_off_location_uuid              String             @db.Uuid
  parcel_route_uuid                   String             @db.Uuid
  drop_off_locations                  drop_off_locations @relation(fields: [drop_off_location_uuid], references: [drop_off_location_uuid], onDelete: Cascade)
  parcel_routes                       parcel_routes      @relation(fields: [parcel_route_uuid], references: [parcel_route_uuid])
}

model parcel_route_first_miles {
  parcel_route_first_mile_uuid String              @id @db.Uuid
  parcel_rate_source_uuid      String              @db.Uuid
  parcel_route_uuid            String              @db.Uuid
  parcel_rate_sources          parcel_rate_sources @relation(fields: [parcel_rate_source_uuid], references: [parcel_rate_source_uuid])
  parcel_routes                parcel_routes       @relation(fields: [parcel_route_uuid], references: [parcel_route_uuid])
}

model parcel_route_last_miles {
  parcel_route_last_mile_uuid String              @id @db.Uuid
  parcel_rate_source_uuid     String              @db.Uuid
  parcel_route_uuid           String              @db.Uuid
  parcel_rate_sources         parcel_rate_sources @relation(fields: [parcel_rate_source_uuid], references: [parcel_rate_source_uuid])
  parcel_routes               parcel_routes       @relation(fields: [parcel_route_uuid], references: [parcel_route_uuid])
}

model drivers {
  driver_uuid            String                  @id @db.Uuid
  first_name             String?                 @db.VarChar(255)
  last_name              String?                 @db.VarChar(255)
  photo                  String?                 @db.VarChar(255)
  email                  String?                 @db.VarChar(255)
  phone                  Json?
  license_number         String?                 @db.VarChar(255)
  sta_license_status     enum_sta_license_status @default(NO)
  sta_number             String?                 @db.VarChar(255)
  sta_expiration_date    DateTime?               @db.Timestamptz(6)
  iacssp_training_date   DateTime?               @db.Timestamptz(6)
  iacssp_expiration_date DateTime?               @db.Timestamptz(6)
  company_uuid           String                  @db.Uuid
  companies              companies               @relation(fields: [company_uuid], references: [company_uuid])
}

model parcel_route_histories {
  user_uuid                 String        @db.Uuid
  comment                   String?
  parcel_route_uuid         String        @db.Uuid
  created_at                DateTime?     @default(now()) @db.Timestamptz(6)
  parcel_route_history_uuid String        @id @db.Uuid
  parcel_routes             parcel_routes @relation(fields: [parcel_route_uuid], references: [parcel_route_uuid])
  users                     users         @relation(fields: [user_uuid], references: [user_uuid])
}

model uk_states_mapping_ups {
  uk_states_mapping_ups_uuid String @id @db.Uuid
  code                       String
  state                      String
}

