generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated/client"
  previewFeatures = ["interactiveTransactions"]
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SequelizeMeta {
  name String @id @db.VarChar(255)
}

model access_policies {
  access_policy_uuid                  String                                @id @db.Uuid
  name                                String                                @db.VarChar(255)
  description                         String                                @db.VarChar(255)
  company_uuid                        String?                               @db.Uuid
  companies                           companies?                            @relation(fields: [company_uuid], references: [company_uuid])
  access_policy_group_access_policies access_policy_group_access_policies[]
  access_policy_permissions           access_policy_permissions[]
  company_user_access_policies        company_user_access_policies[]

  @@unique([name, company_uuid], name: "access_policies_uk")
}

model access_policy_group_access_policies {
  access_policy_group_access_policy_uuid String                @id @db.Uuid
  access_policy_group_uuid               String?               @db.Uuid
  access_policy_uuid                     String?               @db.Uuid
  access_policy_groups                   access_policy_groups? @relation(fields: [access_policy_group_uuid], references: [access_policy_group_uuid])
  access_policies                        access_policies?      @relation(fields: [access_policy_uuid], references: [access_policy_uuid])

  @@unique([access_policy_group_uuid, access_policy_uuid], name: "access_policy_group_access_policies_uk")
}

model access_policy_groups {
  access_policy_group_uuid            String                                @id @db.Uuid
  company_uuid                        String?                               @db.Uuid
  name                                String                                @db.VarChar(255)
  companies                           companies?                            @relation(fields: [company_uuid], references: [company_uuid])
  access_policy_group_access_policies access_policy_group_access_policies[]
  company_user_access_policy_groups   company_user_access_policy_groups[]

  @@unique([name, company_uuid], name: "access_policy_groups_uk")
}

model access_policy_permissions {
  access_policy_permission_uuid String          @id @db.Uuid
  access_policy_uuid            String          @db.Uuid
  permission_uuid               String          @db.Uuid
  resource_uuid                 String          @db.Uuid
  access_policies               access_policies @relation(fields: [access_policy_uuid], references: [access_policy_uuid])
  permissions                   permissions     @relation(fields: [permission_uuid], references: [permission_uuid])
  resources                     resources       @relation(fields: [resource_uuid], references: [resource_uuid])
}

model administrative_division_types {
  administrative_division_type_uuid String                     @id @db.Uuid
  name                              String                     @unique @db.VarChar(255)
  administrative_divisions          administrative_divisions[]
}

model administrative_divisions {
  administrative_division_uuid      String                              @id @db.Uuid
  level                             Int?
  value                             String                              @db.VarChar(255)
  administrative_division_type_uuid String                              @db.Uuid
  administrative_division_types     administrative_division_types       @relation(fields: [administrative_division_type_uuid], references: [administrative_division_type_uuid])
  location_administrative_divisions location_administrative_divisions[]

  @@unique([level, value, administrative_division_type_uuid], name: "level_value_and_type_uk")
}

model air_freight_services {
  air_freight_service_uuid       String                           @id @unique @db.Uuid
  name                           String?                          @unique @db.VarChar(255)
  airline_air_freight_services   airline_air_freight_services[]
  forwarder_air_freight_services forwarder_air_freight_services[]
}

model air_mass_measures {
  air_mass_measure_uuid String       @id @db.Uuid
  name                  String       @db.VarChar(255)
  air_routes            air_routes[]
}

model air_rate_air_rate_custom_fields {
  air_rate_air_rate_custom_field_uuid String                 @id @db.Uuid
  value                               String?
  air_rate_uuid                       String                 @db.Uuid
  air_rate_custom_field_uuid          String                 @db.Uuid
  air_rate_custom_fields              air_rate_custom_fields @relation(fields: [air_rate_custom_field_uuid], references: [air_rate_custom_field_uuid])
  air_rates                           air_rates              @relation(fields: [air_rate_uuid], references: [air_rate_uuid])

  @@unique([air_rate_uuid, air_rate_custom_field_uuid], name: "air_rate_and_air_rate_custom_field_uk")
}

model air_rate_commodities {
  air_rate_commodity_uuid String      @id @db.Uuid
  name                    String      @db.VarChar(255)
  air_rates               air_rates[]
}

model air_rate_custom_fields {
  air_rate_custom_field_uuid      String                            @id @db.Uuid
  field                           String?
  company_uuid                    String                            @db.Uuid
  companies                       companies                         @relation(fields: [company_uuid], references: [company_uuid])
  air_rate_air_rate_custom_fields air_rate_air_rate_custom_fields[]

  @@unique([field, company_uuid], name: "air_company_and_field_uk")
}

model air_rate_types {
  air_rate_type_uuid String      @id @db.Uuid
  name               String      @db.VarChar(255)
  air_rates          air_rates[]
}

model air_rates {
  air_rate_uuid                   String                            @id @db.Uuid
  minimum                         Float?
  fee                             Float?
  profit                          Float?
  minimum_profit                  Float?
  minimum_weight                  Float?
  maximum_weight                  Float?
  air_route_uuid                  String                            @db.Uuid
  air_rate_type_uuid              String?                           @db.Uuid
  air_rate_commodity_uuid         String?                           @db.Uuid
  air_rate_commodities            air_rate_commodities?             @relation(fields: [air_rate_commodity_uuid], references: [air_rate_commodity_uuid])
  air_rate_types                  air_rate_types?                   @relation(fields: [air_rate_type_uuid], references: [air_rate_type_uuid])
  air_routes                      air_routes                        @relation(fields: [air_route_uuid], references: [air_route_uuid])
  air_rate_air_rate_custom_fields air_rate_air_rate_custom_fields[]
}

model air_route_customer_types {
  air_route_customer_type_uuid String        @id @db.Uuid
  air_route_uuid               String        @db.Uuid
  company_type_uuid            String        @db.Uuid
  air_routes                   air_routes    @relation(fields: [air_route_uuid], references: [air_route_uuid])
  company_types                company_types @relation(fields: [company_type_uuid], references: [company_type_uuid])

  @@unique([company_type_uuid, air_route_uuid], name: "air_route_customer_type_uk")
}

model air_route_customers {
  air_route_customer_uuid String     @id @db.Uuid
  air_route_uuid          String     @db.Uuid
  company_uuid            String     @db.Uuid
  air_routes              air_routes @relation(fields: [air_route_uuid], references: [air_route_uuid])
  companies               companies  @relation(fields: [company_uuid], references: [company_uuid])

  @@unique([air_route_uuid, company_uuid], name: "air_route_and_customer_uk")
}

model air_route_week_days {
  week_day_uuid  String     @id @db.Uuid
  week_day       Int?
  air_route_uuid String     @db.Uuid
  air_routes     air_routes @relation(fields: [air_route_uuid], references: [air_route_uuid])
  times          times[]
}

model air_routes {
  air_route_uuid                                       String                     @id @db.Uuid
  effective_on                                         DateTime?                  @db.Timestamptz(6)
  expires_on                                           DateTime?                  @db.Timestamptz(6)
  transit_time_from                                    Int?
  transit_time_to                                      Int?
  notes                                                String?                    @db.VarChar(250)
  restrictions                                         String?                    @db.VarChar(250)
  published                                            Boolean?
  public                                               Boolean?
  company_uuid                                         String                     @db.Uuid
  carrier_company_uuid                                 String?                    @db.Uuid
  carrier_unregistered_company_uuid                    String?                    @db.Uuid
  flight_uuid                                          String?                    @db.Uuid
  air_mass_measure_uuid                                String?                    @db.Uuid
  currency_uuid                                        String?                    @db.Uuid
  air_mass_measures                                    air_mass_measures?         @relation(fields: [air_mass_measure_uuid], references: [air_mass_measure_uuid])
  companies_air_routes_carrier_company_uuidTocompanies companies?                 @relation("air_routes_carrier_company_uuidTocompanies", fields: [carrier_company_uuid], references: [company_uuid])
  unregistered_companies                               unregistered_companies?    @relation(fields: [carrier_unregistered_company_uuid], references: [unregistered_company_uuid])
  companies_air_routes_company_uuidTocompanies         companies                  @relation("air_routes_company_uuidTocompanies", fields: [company_uuid], references: [company_uuid])
  currencies                                           currencies?                @relation(fields: [currency_uuid], references: [currency_uuid])
  flights                                              flights?                   @relation(fields: [flight_uuid], references: [flight_uuid])
  air_rates                                            air_rates[]
  air_route_customer_types                             air_route_customer_types[]
  air_route_customers                                  air_route_customers[]
  air_route_week_days                                  air_route_week_days[]
}

model airline_air_freight_services {
  airline_air_freight_service_uuid String               @id @unique @db.Uuid
  airline_uuid                     String               @db.Uuid
  air_freight_service_uuid         String               @db.Uuid
  air_freight_services             air_freight_services @relation(fields: [air_freight_service_uuid], references: [air_freight_service_uuid])
  airlines                         airlines             @relation(fields: [airline_uuid], references: [airline_uuid])

  @@unique([airline_uuid, air_freight_service_uuid], name: "airline_air_freight_service_uk")
}

model airline_origin_airports {
  airline_origin_airport_uuid String   @id @unique @db.Uuid
  airline_uuid                String   @db.Uuid
  airport_uuid                String   @db.Uuid
  airlines                    airlines @relation(fields: [airline_uuid], references: [airline_uuid])
  airports                    airports @relation(fields: [airport_uuid], references: [airport_uuid])

  @@unique([airline_uuid, airport_uuid], name: "airline_origin_airport_uk")
}

model airlines {
  airline_uuid                 String                         @id @unique @db.Uuid
  airline_prefix               String?                        @db.VarChar(255)
  iata_code                    String?                        @db.VarChar(255)
  company_uuid                 String                         @unique @db.Uuid
  companies                    companies                      @relation(fields: [company_uuid], references: [company_uuid])
  airline_air_freight_services airline_air_freight_services[]
  airline_origin_airports      airline_origin_airports[]
}

model airports {
  airport_uuid                       String                               @id @unique @db.Uuid
  iata_code                          String?                              @unique @db.VarChar(255)
  name                               String?                              @db.VarChar(255)
  location                           String?                              @db.VarChar(255)
  country_uuid                       String?                              @db.Uuid
  countries                          countries?                           @relation(fields: [country_uuid], references: [country_uuid])
  airline_origin_airports            airline_origin_airports[]
  airport_location                   airport_location[]
  exporter_destination_airports      exporter_destination_airports[]
  exporter_origin_airports           exporter_origin_airports[]
  flight_destinations                flight_destinations[]
  flights                            flights[]
  forwarder_origin_airports          forwarder_origin_airports[]
  general_sale_agent_origin_airports general_sale_agent_origin_airports[]
  importer_destination_airports      importer_destination_airports[]
  importer_origin_airports           importer_origin_airports[]
  third_party_airports               third_party_airports[]
}

model api_tokens {
  api_token_uuid String    @id @db.Uuid
  user_uuid      String    @db.Uuid
  token          String?   @db.VarChar(64)
  expires_at     DateTime? @db.Timestamptz(6)
  created_at     DateTime  @db.Timestamptz(6)
  users          users     @relation(fields: [user_uuid], references: [user_uuid])
}

model bank_accounts {
  bank_account_uuid     String                  @id @db.Uuid
  beneficiary_name      String?                 @db.VarChar(255)
  bank_name             String?                 @db.VarChar(255)
  account_number        String?                 @db.VarChar(255)
  swift_code            String?                 @db.VarChar(255)
  iban                  String?                 @db.VarChar(255)
  routing_number        String?                 @db.VarChar(255)
  bank_address_uuid     String?                 @db.Uuid
  currency_uuid         String?                 @db.Uuid
  locations             locations?              @relation(fields: [bank_address_uuid], references: [location_uuid])
  currencies            currencies?             @relation(fields: [currency_uuid], references: [currency_uuid])
  company_bank_accounts company_bank_accounts[]
  user_bank_accounts    user_bank_accounts[]
}

model cargo_types {
  cargo_type_uuid           String                      @id @unique @db.Uuid
  name                      String?                     @unique @db.VarChar(255)
  exporter_cargo_types      exporter_cargo_types[]
  forwarder_cargo_types     forwarder_cargo_types[]
  importer_cargo_types      importer_cargo_types[]
  trucking_cargo_types      trucking_cargo_types[]
  warehouse_3pl_cargo_types warehouse_3pl_cargo_types[]
}

model comments {
  comment_uuid            String                    @id @db.Uuid
  text                    String
  created_at              DateTime                  @db.Timestamptz(6)
  last_update             DateTime?                 @db.Timestamptz(6)
  author_user_uuid        String                    @db.Uuid
  parent_comment_uuid     String?                   @db.Uuid
  file                    String?
  users                   users                     @relation(fields: [author_user_uuid], references: [user_uuid])
  comments                comments?                 @relation("commentsTocomments_parent_comment_uuid", fields: [parent_comment_uuid], references: [comment_uuid])
  other_comments          comments[]                @relation("commentsTocomments_parent_comment_uuid")
  parcel_booking_comments parcel_booking_comments[]
}

model companies {
  company_uuid                                                                       String                                @id @unique @db.Uuid
  legal_name                                                                         String?                               @unique @db.VarChar(255)
  fantasy_name                                                                       String?                               @unique @db.VarChar(255)
  tax_id                                                                             String?                               @unique @db.VarChar(255)
  logo                                                                               String?                               @db.VarChar(255)
  icon                                                                               String?                               @db.VarChar(255)
  currency_uuid                                                                      String?                               @db.Uuid
  alirok_terms_agreement                                                             Boolean?
  user_uuid                                                                          String?                               @db.Uuid
  company_type_uuid                                                                  String?                               @db.Uuid
  onboarding_finished                                                                Boolean?                              @default(false)
  email                                                                              String?                               @db.VarChar(255)
  phone                                                                              Json?                                 @db.Json
  headquarter_address_uuid                                                           String?                               @db.Uuid
  is_registered                                                                      Boolean                               @default(true)
  company_types                                                                      company_types?                        @relation(fields: [company_type_uuid], references: [company_type_uuid])
  currencies                                                                         currencies?                           @relation(fields: [currency_uuid], references: [currency_uuid])
  locations                                                                          locations?                            @relation(fields: [headquarter_address_uuid], references: [location_uuid])
  users                                                                              users?                                @relation(fields: [user_uuid], references: [user_uuid])
  access_policies                                                                    access_policies[]
  access_policy_groups                                                               access_policy_groups[]
  air_rate_custom_fields                                                             air_rate_custom_fields[]
  air_route_customers                                                                air_route_customers[]
  air_routes_air_routes_carrier_company_uuidTocompanies                              air_routes[]                          @relation("air_routes_carrier_company_uuidTocompanies")
  air_routes_air_routes_company_uuidTocompanies                                      air_routes[]                          @relation("air_routes_company_uuidTocompanies")
  airlines                                                                           airlines?
  company_bank_accounts                                                              company_bank_accounts[]
  company_emails                                                                     company_emails[]
  company_integrations                                                               company_integrations[]
  company_relationship_attachments                                                   company_relationship_attachments[]
  company_relationship_custom_fields                                                 company_relationship_custom_fields[]
  company_relationships_companiesTocompany_relationships_addressee_company_uuid      company_relationships[]               @relation("companiesTocompany_relationships_addressee_company_uuid")
  company_relationships_companiesTocompany_relationships_customer_company_uuid       company_relationships[]               @relation("companiesTocompany_relationships_customer_company_uuid")
  company_relationships_companiesTocompany_relationships_requester_company_uuid      company_relationships[]               @relation("companiesTocompany_relationships_requester_company_uuid")
  company_relationships_companiesTocompany_relationships_vendor_company_uuid         company_relationships[]               @relation("companiesTocompany_relationships_vendor_company_uuid")
  company_users                                                                      company_users[]
  drivers                                                                            drivers[]
  drop_off_location_carrier_companies                                                drop_off_location_carrier_companies[]
  drop_off_locations                                                                 drop_off_locations[]
  exporters                                                                          exporters?
  forwarders                                                                         forwarders?
  fuel_surcharge_fees                                                                fuel_surcharge_fees?
  general_sale_agents                                                                general_sale_agents?
  importers                                                                          importers?
  insurances                                                                         insurances?
  parcel_members                                                                     parcel_members[]
  parcel_members_bkp                                                                 parcel_members_bkp[]
  parcel_profit_setup_customers                                                      parcel_profit_setup_customers[]
  parcel_profit_setups_companiesToparcel_profit_setups_company_uuid                  parcel_profit_setups[]                @relation("companiesToparcel_profit_setups_company_uuid")
  parcel_profit_setups_companiesToparcel_profit_setups_vendor_company_uuid           parcel_profit_setups[]                @relation("companiesToparcel_profit_setups_vendor_company_uuid")
  parcel_rate_custom_fields                                                          parcel_rate_custom_fields[]
  parcel_route_customers                                                             parcel_route_customers[]
  parcel_routes_companiesToparcel_routes_carrier_company_uuid                        parcel_routes[]                       @relation("companiesToparcel_routes_carrier_company_uuid")
  parcel_routes_companiesToparcel_routes_company_uuid                                parcel_routes[]                       @relation("companiesToparcel_routes_company_uuid")
  parcel_routes_companiesToparcel_routes_vendor_company_uuid                         parcel_routes[]                       @relation("companiesToparcel_routes_vendor_company_uuid")
  rate_types                                                                         rate_types[]
  relationship_invitations_companiesTorelationship_invitations_customer_company_uuid relationship_invitations[]            @relation("companiesTorelationship_invitations_customer_company_uuid")
  relationship_invitations_companiesTorelationship_invitations_vendor_company_uuid   relationship_invitations[]            @relation("companiesTorelationship_invitations_vendor_company_uuid")
  screening_fees                                                                     screening_fees?
  service                                                                            service[]
  shipping_lines                                                                     shipping_lines?
  third_parties                                                                      third_parties?
  truckings                                                                          truckings?
  value_services                                                                     value_services[]
  value_services_companies                                                           value_services_companies[]
  warehouse_3pls                                                                     warehouse_3pls?
}

model company_bank_accounts {
  user_bank_account_uuid String        @id @db.Uuid
  company_uuid           String        @db.Uuid
  bank_account_uuid      String        @db.Uuid
  bank_accounts          bank_accounts @relation(fields: [bank_account_uuid], references: [bank_account_uuid])
  companies              companies     @relation(fields: [company_uuid], references: [company_uuid])
}

model company_emails {
  company_email_uuid String      @id @db.Uuid
  email_address      String      @db.VarChar(255)
  company_uuid       String      @db.Uuid
  email_type_uuid    String      @db.Uuid
  companies          companies   @relation(fields: [company_uuid], references: [company_uuid])
  email_types        email_types @relation(fields: [email_type_uuid], references: [email_type_uuid])

  @@unique([email_address, company_uuid, email_type_uuid], name: "company_emails_uk")
}

model company_integrations {
  company_integration_uuid String          @id @db.Uuid
  key                      String          @db.VarChar(255)
  company_uuid             String          @db.Uuid
  integration_uuid         String          @db.Uuid
  companies                companies       @relation(fields: [company_uuid], references: [company_uuid])
  integrations             integrations    @relation(fields: [integration_uuid], references: [integration_uuid])
  parcel_routes            parcel_routes[]

  @@unique([company_uuid, integration_uuid], name: "company_and_integration_uk")
}

model company_relationship_attachment_custom_fields {
  company_relationship_attachment_custom_field_uuid String                             @id @db.Uuid
  company_relationship_attachment_uuid              String                             @db.Uuid
  company_relationship_custom_field_uuid            String                             @db.Uuid
  value                                             String                             @db.VarChar(255)
  company_relationship_attachments                  company_relationship_attachments   @relation(fields: [company_relationship_attachment_uuid], references: [company_relationship_attachment_uuid])
  company_relationship_custom_fields                company_relationship_custom_fields @relation(fields: [company_relationship_custom_field_uuid], references: [company_relationship_custom_field_uuid])

  @@unique([company_relationship_attachment_uuid, company_relationship_custom_field_uuid], name: "company_relationship_attachment_and_custom_field_uk")
  @@map("c_r_a_c_f_fkey")
}

model company_relationship_attachments {
  company_relationship_attachment_uuid          String                                          @id @db.Uuid
  notes                                         String?                                         @db.VarChar(255)
  company_relationship_uuid                     String                                          @db.Uuid
  company_uuid                                  String                                          @db.Uuid
  company_relationships                         company_relationships                           @relation(fields: [company_relationship_uuid], references: [company_relationship_uuid])
  companies                                     companies                                       @relation(fields: [company_uuid], references: [company_uuid])
  company_relationship_attachment_custom_fields company_relationship_attachment_custom_fields[]

  @@unique([company_relationship_uuid, company_uuid], name: "company_relationship_and_company_uk")
}

model company_relationship_custom_fields {
  company_relationship_custom_field_uuid        String                                          @id @db.Uuid
  field                                         String                                          @unique @db.VarChar(255)
  display                                       Boolean?
  company_uuid                                  String?                                         @db.Uuid
  companies                                     companies?                                      @relation(fields: [company_uuid], references: [company_uuid])
  company_relationship_attachment_custom_fields company_relationship_attachment_custom_fields[]
}

model company_relationships {
  company_relationship_uuid                                         String                               @id @db.Uuid
  created_at                                                        DateTime?                            @db.Timestamptz(6)
  active                                                            Boolean?                             @default(true)
  contact_person                                                    String?                              @db.VarChar(255)
  credit_line                                                       Float?
  payment_term                                                      String?                              @db.VarChar(255)
  customer_company_uuid                                             String                               @db.Uuid
  vendor_company_uuid                                               String                               @db.Uuid
  requester_company_uuid                                            String                               @db.Uuid
  addressee_company_uuid                                            String                               @db.Uuid
  currency_uuid                                                     String?                              @db.Uuid
  connected                                                         enum_company_relationships_connected @default(PENDING)
  companies_companiesTocompany_relationships_addressee_company_uuid companies                            @relation("companiesTocompany_relationships_addressee_company_uuid", fields: [addressee_company_uuid], references: [company_uuid])
  currencies                                                        currencies?                          @relation(fields: [currency_uuid], references: [currency_uuid])
  companies_companiesTocompany_relationships_customer_company_uuid  companies                            @relation("companiesTocompany_relationships_customer_company_uuid", fields: [customer_company_uuid], references: [company_uuid])
  companies_companiesTocompany_relationships_requester_company_uuid companies                            @relation("companiesTocompany_relationships_requester_company_uuid", fields: [requester_company_uuid], references: [company_uuid])
  companies_companiesTocompany_relationships_vendor_company_uuid    companies                            @relation("companiesTocompany_relationships_vendor_company_uuid", fields: [vendor_company_uuid], references: [company_uuid])
  company_relationship_attachments                                  company_relationship_attachments[]

  @@unique([customer_company_uuid, vendor_company_uuid], name: "company_relationship_uk")
}

model company_types {
  company_type_uuid                  String                               @id @unique @db.Uuid
  name                               String                               @unique @db.VarChar(255)
  display                            Boolean?
  air_route_customer_types           air_route_customer_types[]
  companies                          companies[]
  parcel_profit_setup_customer_types parcel_profit_setup_customer_types[]
  parcel_route_customer_types        parcel_route_customer_types[]
  value_services_company_types       value_services_company_types[]
}

model company_user_access_policies {
  company_user_access_policy_uuid String          @id @db.Uuid
  company_user_uuid               String          @db.Uuid
  access_policy_uuid              String          @db.Uuid
  access_policies                 access_policies @relation(fields: [access_policy_uuid], references: [access_policy_uuid])
  company_users                   company_users   @relation(fields: [company_user_uuid], references: [company_user_uuid])
}

model company_user_access_policy_groups {
  company_user_access_policy_group_uuid String               @id @db.Uuid
  company_user_uuid                     String               @db.Uuid
  access_policy_group_uuid              String               @db.Uuid
  access_policy_groups                  access_policy_groups @relation(fields: [access_policy_group_uuid], references: [access_policy_group_uuid])
  company_users                         company_users        @relation(fields: [company_user_uuid], references: [company_user_uuid])

